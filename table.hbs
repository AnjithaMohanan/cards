<section>

    <div class="row">

     

        <div class="col-md-12">


            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Category</th>
                        <th scope="col">Descriptions</th>
                    </tr>
                </thead>
                {{#each products}}
                <tbody>
                    <tr>
                        <th scope="row">1</th>
                        <td>{{this.name}}</td>
                        <td>{{this.category}}</td>
                        <td>{{this.description}}</td>
                    </tr>
                </tbody>
                {{/each}}


            </table>



        </div>

      

    </div>



</section>